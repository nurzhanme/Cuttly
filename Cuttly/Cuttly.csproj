    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <RepositoryUrl>https://github.com/nurzhanme/Cuttly</RepositoryUrl>
    <PackageProjectUrl>https://github.com/nurzhanme/Cuttly</PackageProjectUrl>
    <Authors>Nurzhan AItbayev</Authors>
